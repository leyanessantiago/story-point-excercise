    "react-native-screens": "~2.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/expect": "^24.3.0",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@types/react-redux": "^7.1.7",
